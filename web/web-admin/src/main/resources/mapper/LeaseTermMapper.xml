<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gy.lease.web.admin.mapper.LeaseTermMapper">

    <select id="selectInfo" resultType="com.gy.lease.model.entity.LeaseTerm">
        select id, month_count, unit
        from lease_term
        where id in (select lease_term_id
                     from room_lease_term
                     where room_id = #{id}
                       and is_deleted = 0)
          and is_deleted = 0
    </select>
</mapper>
