<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gy.lease.web.app.mapper.GraphInfoMapper">

    <select id="selectListByItemTypeAndId" resultType="com.gy.lease.web.app.vo.graph.GraphVo">
        select id, name, item_type, item_id, url
            from graph_info
        where is_deleted = 0
        and item_type = #{itemType}
        and item_id = #{id}
    </select>
</mapper>
